{
  "name": "@0xbigboss/vite-console-forward-plugin",
  "version": "2.2.0",
  "description": "A Vite plugin that forwards browser console logs to the Vite dev server console",
  "main": "dist/vite-console-forward-plugin.js",
  "types": "dist/vite-console-forward-plugin.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "format": "npx prettier --write .",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report"
  },
  "keywords": [
    "vite",
    "plugin",
    "console",
    "logging",
    "development",
    "debugging"
  ],
  "author": "0xBigBoss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xBigBoss/vite-console-forward-plugin.git"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^24.0.7",
    "playwright": "^1.53.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "vite": "^7.0.0"
  },
  "dependencies": {
    "micromatch": "^4.0.8"
  }
}
